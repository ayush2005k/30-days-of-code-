

import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        // Predefined variables
        int i = 4;
        double d = 4.0;
        String s = "HackerRank ";

        // Declare variables
        int myInt;
        double myDouble;
        String myString;

        // Read inputs
        Scanner scan = new Scanner(System.in);

        // Read and save an integer
        myInt = scan.nextInt();
        // Read and save a double
        myDouble = scan.nextDouble();
        scan.nextLine(); // Consume newline
        // Read and save a string
        myString = scan.nextLine();

        // Perform operations and print results
        System.out.println(i + myInt); // Sum of integers
        System.out.printf("%.1f\n", d + myDouble); // Sum of doubles with one decimal precision
        System.out.println(s + myString); // Concatenate strings
    }
}
